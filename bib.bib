  title     = {Reliability of the Music in Everyday Life (MEL) Scale: A Parent-Report Assessment for Children on the Autism Spectrum},
  journal   = {Journal of Music Therapy},
  year      = {2018},
  volume    = {55},
  number    = {2},
  pages     = {133--155},
  doi       = {10.1093/jmt/thy002},
  publisher = {Oxford University Press},
  language  = {English},
  abstract  = {For young children on the autism spectrum, the inclusion of shared parent-child music activities in everyday life may provide additional opportunities for social interactions in the home. This study developed and tested the reliability of a self-report assessment to measure the use of Music in Everyday Life (MEL) by parents with young children on the autism spectrum. The study confirmed the reliability of two predetermined subscales: Music in Everyday Life-Joint Activities using Music (MEL-JAM) and Music in Everyday Life-Routine Activities using Music (MEL-RAM). Internal consistency and positive item-total correlation were demonstrated, confirming the reliability of the MEL assessment to measure the use of music in everyday life.},
  keywords  = {Autism, Music Therapy, Psychometrics, Reliability, Parent-Report Assessment},
  doi       = {10.1093/jmt/thy002}
}
